{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "esnext"
  },
  "include": ["next-env.d.ts", "**/*.js", "**/*.jsx", "**/*.mjs"],
  "exclude": ["node_modules"]
}
